{
  "name": "balticrobo/website",
  "description": "Baltic Robot Battles event website",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "cocur/slugify": "^3.2",
    "doctrine/doctrine-bundle": "^1.11",
    "doctrine/doctrine-migrations-bundle": "^2.0",
    "doctrine/orm": "^2.6",
    "dompdf/dompdf": "^0.8.3",
    "php-http/guzzle6-adapter": "^2.0",
    "php-http/httplug-bundle": "^1.15",
    "ramsey/uuid": "^3.8",
    "ramsey/uuid-doctrine": "^1.5",
    "sensio/framework-extra-bundle": "^5.3",
    "sentry/sentry-symfony": "^3.0",
    "symfony/asset": "4.2.*",
    "symfony/console": "4.2.*",
    "symfony/dotenv": "4.2.*",
    "symfony/expression-language": "4.2.*",
    "symfony/flex": "^1.2",
    "symfony/form": "4.2.*",
    "symfony/framework-bundle": "4.2.*",
    "symfony/monolog-bundle": "^3.3",
    "symfony/security-bundle": "4.2.*",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/translation": "4.2.*",
    "symfony/twig-bundle": "4.2.*",
    "symfony/validator": "4.2.*",
    "symfony/yaml": "4.2.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "jakub-onderka/php-var-dump-check": "dev-master#eef28e2d7bf71ca1530243dbba5939d9eae09aaf",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/debug-bundle": "4.2.*",
    "symfony/var-dumper": "4.2.*",
    "symfony/web-profiler-bundle": "4.2.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BalticRobo\\Website\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BalticRobo\\Website\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "rm -rf .git/hooks && ln -s ../.misc/hooks .git/hooks": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.2.*"
    }
  }
}
