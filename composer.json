{
  "name": "balticrobo/website",
  "description": "Baltic Robot Battles event website",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/doctrine-migrations-bundle": "^1.3",
    "doctrine/orm": "^2.6",
    "sensio/framework-extra-bundle": "^5.1",
    "sentry/sentry-symfony": "^2.0",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "^1.1",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/monolog-bundle": "^3.2",
    "symfony/security-bundle": "*",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/webpack-encore-pack": "^1.0",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.11",
    "jakub-onderka/php-var-dump-check": "dev-master#7d012745d012edeace62d509786ac7c3498c9c57",
    "squizlabs/php_codesniffer": "^3.2",
    "symfony/debug-bundle": "^4.0",
    "symfony/var-dumper": "^4.0",
    "symfony/web-profiler-bundle": "^4.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BalticRobo\\Website\\": "src/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "rm -rf .git/hooks && ln -s ../.misc/hooks .git/hooks" : "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "^4.1"
    }
  }
}
